{
  "name": "ray-so-export",
  "title": "Ray.so Export",
  "description": "Export code snippets to ray.so",
  "icon": "command-icon.png",
  "author": "you",
  "categories": [
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
